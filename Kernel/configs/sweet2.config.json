[
    {
      "kernelSource": {
        "name": "sweet2",
        "repo": "https://github.com/Lafactorial/kernel_xiaomi_sm6150.git",
        "branch": "lineage-21",
        "device": "sweet2",
        "defconfig": "sweet2_defconfig"
      },
      "toolchains": [
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86",
        "branch": "android-14.0.0_r53",
        "name": "tc-clang",
        "binaryEnv": [
          "clang-r487747c/bin",
          "llvm-binutils-stable"
        ]
      },
      {
        "repo": "https://github.com/LineageOS/android_prebuilts_tools-lineage",
        "branch": "lineage-21.0",
        "name": "tc-tools",
        "binaryEnv": [
          "linux-x86/bin"
        ]
      },
      {
        "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_arm_arm-linux-androideabi-4.9",
        "branch": "lineage-19.1",
        "name": "tc-gcc",
        "binaryEnv": [
            "bin"
        ]
      },
      {
        "repo": "https://android.googlesource.com/kernel/prebuilts/build-tools",
        "branch": "android-14.0.0_r0.53",
        "name": "tc-kernel-build-tools",
        "binaryEnv": [
            "linux-x86/bin"
        ]
      },
      {
        "repo": "https://github.com/crdroidandroid/android_prebuilts_build-tools",
        "branch": "14.0",
        "name": "tc-build-tools",
        "binaryEnv": [
            "linux-x86/bin"
        ]
      },
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/misc",
        "branch": "android-14.0.0_r53",
        "name": "tc-misc",
        "binaryEnv": [
            "linux-x86/dtc",
            "linux-x86/libufdt"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "HOSTCC": "tc-clang/clang-r487747c/bin/clang",
        "HOSTCXX": "tc-clang/clang-r487747c/bin/clang++",
        "LD": "tc-clang/clang-r487747c/bin/ld.lld",
        "AR": "tc-clang/clang-r487747c/bin/llvm-ar",
        "LLVM": "1",
        "LLVM_IAS": "1",
        "LZ4": "tc-kernel-build-tools/linux-x86/bin/lz4",
        "LEX": "tc-build-tools/linux-x86/bin/flex",
        "YACC": "tc-build-tools/linux-x86/bin/bison",
        "M4": "tc-build-tools/linux-x86/bin/m4",
        "CROSS_COMPILE": "tc-gcc/bin/aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "tc-gcc/bin/arm-linux-androidkernel-",
        "CROSS_COMPILE_COMPAT": "tc-gcc/bin/arm-linux-androidkernel-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
